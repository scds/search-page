---
layout: none
---
[
{% for workshop in workshops %}
  {
    "title": "{{ workshop.Title | strip_newlines | remove: '"' }}",
    "year" : "{{ workshop.Year }}",
    "url"  : "{{ workshop.URL }}",
    "image": "{{ workshop.Image }}",
    "series": "{{ workshop.Series }}",
    "type" : "{{ workshop.Type }}",
    "topic": "{{ workshop.Topic }}",
    "software": "{{ workshop.Software }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
